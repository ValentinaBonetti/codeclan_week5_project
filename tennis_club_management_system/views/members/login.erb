<h1>enter the members section with your credentials</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th>Type</th>
  </tr>
  <% for member in @members %>
  <tr>
    <td><a href="/members/<%= member.id %>/index"><%= "#{member.first_name} #{member.last_name}" %></a></td>
    <td><%= member.membership_type %></td>
  </tr>
  <% end %>
</table>
