<h1>all the tennis classes!</h1>

<table>
  <tr>
    <th>CLASS</th>
    <th>DATE</th>
    <th>TIME</th>
    <th></th>
  </tr>
  <% for tennis_class in @tennis_classes %>
  <tr>
    <td><%= tennis_class.name %></td>
    <td><%= tennis_class.class_date %></td>
    <td><%=tennis_class.class_time %></td>
    <td><a href="/members/<%= @member.id %>/<%= tennis_class.id %>/<%= tennis_class.court_id %>/book_class">book</a></td>
  </tr>
  <% end %>
</table>
