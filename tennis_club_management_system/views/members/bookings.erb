<h1>all my bookings</h1>

<h4>CLASSES</h4>
<div id="member_class_bookings_table">
<table >
  <tr>
    <th>ID</th>
    <th>CLASS</th>
    <th>COURT</th>
    <th>DATE</th>
    <th>TIME</th>
    <th>DURATION</th>
    <th></th>
  </tr>
  <% for booking in @mybookings %>
  <tr>
    <td><%= booking.id %></td>
    <td><%= booking.tennis_class.name %></td>
    <td><%=booking.court_number %></td>
    <td><%=booking.booking_date %></td>
    <td><%=booking.booking_time %></td>
    <td><%=booking.booking_duration %></td>
  </tr>
  <% end %>
</table>
</div>

<h4>COURT BOOKINGS</h4>
<div id="member_court_bookings_table">

<table>
  <tr>
    <th>ID</th>
    <th>COURT</th>
    <th>DATE</th>
    <th>TIME</th>
    <th>DURATION</th>
    <th></th>
  </tr>
  <% for booking in @mycourtbookings %>
  <tr>
    <td><%= booking.id %></td>
    <td><%=booking.court_number %></td>
    <td><%=booking.booking_date %></td>
    <td><%=booking.booking_time %></td>
    <td><%=booking.booking_duration %></td>

  </tr>
  <% end %>
</table>
</div>
